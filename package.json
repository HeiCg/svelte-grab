{
  "name": "svelte-grab",
  "version": "0.1.1",
  "description": "Click any element to get component stack with source locations. Makes coding agents 3x faster for UI changes.",
  "keywords": [
    "svelte",
    "svelte5",
    "devtools",
    "developer-experience",
    "coding-agent",
    "claude",
    "cursor",
    "ai-coding"
  ],
  "license": "MIT",
  "author": "Quality AI Solutions",
  "repository": {
    "type": "git",
    "url": "https://github.com/qualityaisolutions/svelte-grab"
  },
  "type": "module",
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "svelte-package -i src/lib -o dist",
    "prepublishOnly": "npm run build",
    "check": "svelte-check --tsconfig ./tsconfig.json"
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "devDependencies": {
    "@sveltejs/package": "^2.3.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "typescript": "^5.0.0",
    "vite": "^6.0.0"
  }
}
